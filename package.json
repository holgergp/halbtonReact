{
  "name": "halbton-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://halbton.netlify.app/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.14.6",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "cypress": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "tsc": "node_modules/.bin/tsc",
    "lint": "node_modules/.bin/eslint ./ --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "./node_modules/.bin/prettier --single-quote --write \"+(src|cypress)/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier-check": "prettier --single-quote --check \"+(src|cypress)/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "run-s prettier-check tsc lint test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "+(src|cypress)/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write"
    ]
  }
}
